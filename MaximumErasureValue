class Solution:
    def maximumUniqueSubarray(self, nums: List[int]) -> int:
        
        # slide window
        
        mydic = {0:0}
        total, maxNum, mark = 0, 0, 0
        
        for n in nums:
            total += n
            if n in mydic and mydic[n] >= mark:
                mark = mydic[n]
            maxNum = max(maxNum, total - mark)
            mydic[n] = total
        return maxNum
